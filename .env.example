# PostgreSQL
POSTGRES_DB=boxinvest
POSTGRES_USER=boxinvest
POSTGRES_PASSWORD=boxinvest

# Backend (set automatically by docker-compose, override here for local dev)
DATABASE_URL=postgresql+asyncpg://boxinvest:boxinvest@localhost:5432/boxinvest

# Frontend
VITE_API_URL=http://localhost:8000

# Scraper settings
SCRAPER_REQUEST_DELAY_SECONDS=2
SCRAPER_MAX_LISTINGS_PER_RUN=50

# ML
ML_MODEL_PATH=/app/models/price_estimator.pkl
ML_RETRAIN_MIN_SAMPLES=100
